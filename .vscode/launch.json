  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest All",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "cwd": "${workspaceRoot}",
      "args": [
          "--i",
          "--config",
          "jest.json"
      ],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest File",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "cwd": "${workspaceRoot}",
      "args": [
          "--i",
          "--config",
          "jest.json",
          "${file}"
      ],
      "sourceMaps": true
    },
  ]
}
